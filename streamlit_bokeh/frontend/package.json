{
  "name": "streamlit_bokeh",
  "license": "Apache-2.0",
  "version": "3.7.2",
  "private": true,
  "dependencies": {
    "@bokeh/bokehjs": "^3.7.3",
    "@streamlit/component-v2-lib": "^0.1.0",
    "color2k": "^2.0.3"
  },
  "scripts": {
    "start": "vite",
    "dev": "DEV=1 vite build --watch",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest --run",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "yarn clean && yarn typecheck && vite build --mode production",
    "clean": "rm -rf build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^22.14.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.6.0"
}
