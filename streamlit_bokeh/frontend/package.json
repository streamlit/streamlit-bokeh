{
  "name": "streamlit_bokeh",
  "license": "Apache-2.0",
  "version": "3.8.0",
  "private": true,
  "dependencies": {
    "@bokeh/bokehjs": "^3.8.0",
    "@streamlit/component-v2-lib": "^0.1.0",
    "color2k": "^2.0.3",
    "streamlit-component-lib": "^2.0.0"
  },
  "scripts": {
    "build:v1": "yarn clean && yarn typecheck && cross-env NODE_ENV=production vite build --config vite.v1.config.ts && yarn copy:bokeh && yarn copy:bokeh",
    "build:v2": "yarn clean && yarn typecheck && cross-env NODE_ENV=production vite build && yarn copy:bokeh",
    "build": "yarn clean && yarn typecheck && cross-env NODE_ENV=production vite build --config vite.v1.config.ts && cross-env NODE_ENV=production vite build && yarn copy:bokeh",
    "clean": "rimraf build",
    "copy:bokeh": "mkdir -p build && cp -R public/bokeh build/",
    "dev:v1": "cross-env DEV=1 NODE_ENV=development vite --config vite.v1.config.ts",
    "dev": "cross-env DEV=1 NODE_ENV=development vite build --watch",
    "format": "prettier --write src/**/*.ts vite.config.ts",
    "test:dev": "vitest",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": ".",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "cross-env": "^10.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.6.0"
}
