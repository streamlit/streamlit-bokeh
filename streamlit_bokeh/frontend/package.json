{
  "name": "streamlit_bokeh",
  "license": "Apache-2.0",
  "version": "3.8.0",
  "private": true,
  "dependencies": {
    "@bokeh/bokehjs": "^3.8.0",
    "@streamlit/component-v2-lib": "^0.1.0",
    "color2k": "^2.0.3"
  },
  "scripts": {
    "build": "yarn clean && yarn typecheck && NODE_ENV=production vite build",
    "clean": "rm -rf build",
    "dev": "NODE_ENV=development vite build --watch",
    "format": "prettier --write src/**/*.ts vite.config.ts",
    "test:dev": "vitest",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": ".",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.150",
    "@types/node": "^22.14.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.6.0"
}
